{
    "name":"theme_changer",
    "description":"theme changer",
    "version":"1.0.0",
    "manifest_version":2,
    "background":{
        "scripts":[
            "popup.js"
        ],
        "persistent":true
    },
    "browser_action":{
        "default_title":"theme changer",
        "default_popup":"popup.html"
    },
    "permissions":[
        "https://*/*",
        "http://*/*",
        "tabs"
    ]
}